  "panels": [
    {
      "title": "n8n — Workflow Executions / 5m",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(n8n_workflow_execution_total[5m])",
          "legendFormat": "{{status}}"
        }
      ]
    },
    {
      "title": "FastAPI — Request Rate / 5m",
      "type": "timeseries",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "rate(http_requests_total{service=\"fastapi\"}[5m])",
          "legendFormat": "{{method}} {{path}}"
        }
      ]
    },
    {
      "title": "FastAPI — p95 Latency",
      "type": "gauge",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{service=\"fastapi\"}[5m]))"
        }
      ]
    },
    {
      "title": "n8n — Active Executions",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 8 },
      "targets": [
        {
          "expr": "n8n_running_executions_total"
        }
      ]
    },
    {
      "title": "FastAPI — 5xx Error Rate",
      "type": "stat",
      "datasource": "Prometheus",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 8 },
      "targets": [
        {
          "expr": "rate(http_requests_total{service=\"fastapi\", status=~\"5..\"}[5m])"
        }
      ]
    }
  ],
